{
  "name": "express-typescript-starter",
  "version": "1.0.0",
  "private": false,
  "author": "Osama Riaz <riazosama@gmail.com>",
  "scripts": {
    "start": "npm run build && yarn run serve",
    "test": "npm run build && rimraf tests/sqlite.db && NODE_ENV=test jest --forceExit --verbose dist/tests/*.test.js",
    "build": "npm run clean && tsc",
    "serve": "tsc-watch --outDir ./dist --onSuccess \"node ./dist/app.js\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && yarn run serve-debug",
    "serve-debug": "tsc-watch --outDir ./dist --onSuccess \"node --inspect=9222 --trace-warnings ./dist/app.js\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "aws-sdk": "^2.536.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-roles": "^3.1.2",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-limiter": "^1.6.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "oauth2orize": "^1.11.0",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-http-oauth": "^0.1.3",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.5.0",
    "passport-oauth2-client-password": "^0.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "sequelize": "^5.19.0",
    "sequelize-typescript": "^1.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.1",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/passport": "^1.0.1",
    "@types/sequelize": "^4.28.4",
    "ts-node": "^8.4.1",
    "tsc-watch": "^4.0.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
}
